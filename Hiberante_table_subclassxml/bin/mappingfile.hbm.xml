<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

        
   
 
  
  
 <hibernate-mapping>  
 <class name="com.mypack4.College" table="collegex100">
 <id name="cid">
 <generator class="increment"></generator>
 </id>
 
 <property name="cname"></property>
 
 <joined-subclass name="com.mypack4.Branch1" table="branchx100">
 <Key column="id1"></Key>
 <property name="bid"></property>
 <property name="bname"></property> 
 </joined-subclass>
 
 <joined-subclass name="com.mypack4.Branch2" table="branchx200">
 <Key column="id1"></Key>
 <property name="bid2"></property>
 <property name="bname2"></property>
 </joined-subclass>

 
 </class>
            
 </hibernate-mapping>  